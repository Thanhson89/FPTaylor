needs "build_tform2.hl";;

open Proof;;
open List;;
open Build_tform2;;

let path = "../benchmarks/proofs/proofs";;

let load name =
  let fname = Filename.concat path name in
    load_proof fname;;

let pp = 12;;

let proof = load "i6.proof";;
let tf = snd (hd (build_tform pp proof));;
let result = prove_bound pp proof tf;;


